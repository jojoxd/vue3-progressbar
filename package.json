{
  "name": "@jojoxd/vue-progressbar",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && tsc --emitDeclarationOnly --outDir dist",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^2.0.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue": "^3.1.1",
    "vue-tsc": "^0.0.24"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-progressbar.umd.js",
  "module": "./dist/vue-progressbar.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-progressbar.es.js",
      "require": "./dist/vue-progressbar.umd.js"
    }
  }
}
